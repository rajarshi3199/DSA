class Solution:
    def maximizeSquareHoleArea(self, n: int, m: int, hBars: list[int], vBars: list[int]) -> int:
        
        def get_max_gap(bars):
            bars.sort()
            max_consecutive = 1
            current_consecutive = 1
            
            # Find the longest streak of consecutive integers
            for i in range(1, len(bars)):
                if bars[i] == bars[i-1] + 1:
                    current_consecutive += 1
                else:
                    max_consecutive = max(max_consecutive, current_consecutive)
                    current_consecutive = 1
            
            max_consecutive = max(max_consecutive, current_consecutive)
            # A streak of k bars creates a gap of k + 1 units
            return max_consecutive + 1

        # Calculate max possible gap for horizontal and vertical
        max_h = get_max_gap(hBars)
        max_v = get_max_gap(vBars)
        
        # The side of the square is the smaller of the two gaps
        side = min(max_h, max_v)
        
        return side * side
